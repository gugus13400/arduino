const int un=2;
const int deux=3;
const int trois=4;
const int quatre=5;
const int cinq=6;
const int six=7;
const int sept=8;

void setup()
{
pinMode(un,OUTPUT);
pinMode(deux,OUTPUT);
pinMode(trois,OUTPUT);
pinMode(quatre,OUTPUT);
pinMode(cinq,OUTPUT);
pinMode(six,OUTPUT);
pinMode(sept,OUTPUT);
}

void loop()
{
digitalWrite(un,HIGH);
digitalWrite(deux,HIGH);
digitalWrite(trois,HIGH);
digitalWrite(quatre,HIGH);
digitalWrite(cinq,HIGH);
digitalWrite(six,HIGH);
digitalWrite(sept,LOW);


}

